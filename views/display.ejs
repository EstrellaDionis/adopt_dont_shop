<div class="form-group row">
  <label class="col-form-label col-sm-2" for="name">Name</label>
  <div class="col-sm-8">
    <input readonly class="form-control" id="name" value="<%= model.name %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="type">Type</label>
  <div class="col-sm-7">
    <input readonly class="form-control" id="type" value="<%= model.type %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="breed">Breed</label>
  <div class="col-sm-10">
    <textarea
      readonly
      class="form-control"
      cols="20"
      id="breed"
      maxlength="32000"
      rows="7"
    >
<%= model.breed %></textarea
    >
  </div>
</div>
